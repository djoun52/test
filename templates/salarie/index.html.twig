


{% extends 'base.html.twig' %}

{% block title %}Hello SalariController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
  

   
{% endblock %}

{% block contenu %}

<table>
    <thead>
        <tr>
            <th>SALARIE  </th>
            <th>ADRESSE  </th>
            <th>CP  </th>
            <th>VILLE  </th>
            <th>DATE EMBAUCHE  </th>
            <th>ENTRERISE  </th>
        </tr>
    </thead>
    <tbody>
        (% for salarie in salarie %)
        <tr>
            <td> <a href=" ({ path{'salarie.show', {'id':salarie.id} })" > </td>
            <td>({ salarie.adressee})</td>
            <td>({ salarie.cp})</td>
            <td>({ salarie.ville})</td>
            <td>({ salarie.dateEmbauche | date('d/m/y') })</td>
            <td>({ salarie.entreprise.raisonSocial})</td>
        </tr>
    </tbody>
</table>
{% endblock %}